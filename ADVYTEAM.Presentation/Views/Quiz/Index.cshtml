@using ADVYTEAM.Presentation.Models
@model QuizSelectionVM

@{
    ViewBag.Title = "Quiz Selection Page";
}

<h2>Quiz Selection</h2>

@using (Html.BeginForm())
{

    @Html.DropDownList("SelectedCategory", (SelectList)ViewData["cats"],null, @id = "D")

    @Html.DropDownList("SelectedSkill", (SelectList)ViewData["skills"])

    <div class="form-group">
        <div class="col-md-offset-2 col-md-10">
            <input type="submit" value="Create" class="btn btn-default" />
        </div>
    </div>
}